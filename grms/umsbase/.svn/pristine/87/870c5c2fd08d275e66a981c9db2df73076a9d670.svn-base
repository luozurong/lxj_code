package com.hori.vo;

import java.io.Serializable;

/**
 * 上传的文件vo
 * @author laizs
 * @time 2014-5-26 下午2:20:09
 * @file ImagePathVo.java
 */
public class FilePathVo implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	//id
	private String id ="";
	// 原图访问路径
	private String path_url = "";
	// 名称
	private String name = "";
	//文件大小
	private String fileSize;
	
	//文件前缀  如  aa.jpg --->aa
	private String prefixName;
	
	//后缀名称 如  aa.jpg --->jpg
	private String suffixName;
	
	
	
	@Override
	public String toString() {
		return "FilePathVo [path_url=" + path_url + ", name=" + name + ", fileSize=" + fileSize + ", prefixName="
				+ prefixName + ", suffixName=" + suffixName + "]";
	}
	public String getPrefixName() {
		return prefixName;
	}
	public void setPrefixName(String prefixName) {
		this.prefixName = prefixName;
	}
	public String getSuffixName() {
		return suffixName;
	}
	public void setSuffixName(String suffixName) {
		this.suffixName = suffixName;
	}
	public String getFileSize() {
		return fileSize;
	}
	public void setFileSize(String fileSize) {
		this.fileSize = fileSize;
	}
	public String getPath_url() {
		return path_url;
	}
	public void setPath_url(String path_url) {
		this.path_url = path_url;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
        public String getId() {
            return id;
        }
        public void setId(String id) {
            this.id = id;
        }
	
}
