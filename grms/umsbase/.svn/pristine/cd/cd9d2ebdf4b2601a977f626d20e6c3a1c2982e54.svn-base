html,body {
    background-color: #f8f8f8;
}

.workorder-breadcrumb{
    margin-left:24px;
    min-width: 1130px;
}

.workorder-breadcrumb{
    line-height: 50px;
}
.workorder-breadcrumb-icon{
    display: inline-block;
    width:16px;
    height:22px;
    float:left;
    margin-top:15px;
    margin-right:10px;
    background:url(../images/ic_location.png) no-repeat;
}
.system-set{
    box-sizing: border-box;
    margin: 20px 24px 60px 25px;
    line-height:50px;
    background: #fff;
    color:#fff;
    border: 1px solid #d7d7d7;
    box-shadow: 0 2px 2px #eee;
}
.system-setWord{
    padding-left:24px;
    line-height:50px;
    height:50px;
    background: #4d525a;
    color:#fff;
}
.system-setChoose{
    color:#666;
    padding-top:15px;
    padding-left:25px;
    min-width: 900px;
    height: 540px;
}
.system-setChoose>div>span{
    display: inline-block;
    width: 385px;
    color:#222;
}
.system-setChoose>div>label{
    display: inline-block;
}
.system-setShow{
    margin-right: 150px;

}
.system-setChoose>div>label input{
    margin-top:17px;
    margin-right:12px;
    float:left;
    display: inline-block;
    width:16px;
    height:16px;
    border-radius: 100%;
}
