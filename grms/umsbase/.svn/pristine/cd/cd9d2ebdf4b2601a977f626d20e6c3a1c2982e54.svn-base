.system-set input[type=checkbox]:checked::after ,input[type=radio]:checked::after{
    content: url(../images/inputradio.png);
    display: block;
    position: absolute;
    top: -1px;
    right: -1px;
}


.time-alarm {
    margin-left: 24px;
    margin-right: 25px;
}
.title1 {
    padding-left: 24px;
    height: 50px;
    line-height: 50px;
    font-size: 14px;
    color: #ffffff;
    background-color: #4d525a;
}
.alarm-body {
    height: 160px;
    background: #fff;
    border: 1px solid #d7d7d7;
    box-shadow: 0 2px 2px #eee;
}
.setips {
    margin-left: 26px;
    margin-top: 20px;
    height: 34px;
    line-height: 34px;
    font-size: 12px;
    color: #222222;
}
.select1 {
    margin-left: 10px;
    margin-right: 10px;
}
.combo {
    width: 80px !important;
    height: 32px;
    line-height: 34px;
}
.textbox .textbox-text{
    width: 60px !important;
    text-align: center;

}
.comfirm {
    margin:48px auto;
    width: 120px;
    height: 40px;
    line-height: 38px;
    text-align: center;
    font-size: 14px;
    background-color: #51d2b6;
    color: #FFFFFF;
    border:1px solid #51d2b6;
    border-radius: 4px;
}
.comfirm1{
    margin-top: 20px;
}

.time-over {
    margin-top: 24px;
}
.time-over .alarm-body{
    height: 276px;
}