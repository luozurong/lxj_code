.north{
	height:60px;
	background-color: #383D41;
	border: none;
	overflow: hidden;
}
.logoPic{
	margin: 10px 0 0 50px;
}
.mesIcon{
	display: inline-block;
	width: 20px;
	height: 60px;
	line-height: 60px;
	padding: 0 20px;
	position: relative;
	/*line-height: 60px;*/
	/*background: url(../../common/images/ic_message.png) left center no-repeat;*/
}
.mesIcon img{
	cursor: pointer;
}
.mesNum{
	display: inline-block;
	width: 22px;
	height:22px;
	color:#FFFFFF ;
	background-color: #ff8c40;
	line-height: 22px;
	font-size: 12px;
	text-align: center;
	position: absolute;
	border-radius: 50%;
	top:12px;
	left: 32px;
}
/*左栏目背景图片*/
.logo-img-black,.logo-img-white {
    position: absolute;
    left: 20px;
    top: 15px;
    width: 20px;
    height: 20px;
}
.logo-img-white {
    display: none;
}
.logo-img>img {
    display: block;
    width: 100%;
    height: 100%;
}
.admin img{
	margin-right: 5px;
}
img{
	vertical-align: middle;
}
.admin{
	display: inline-block;
	color: #FFFFFF;
	/*width: 20px;*/
	line-height: 60px;
	font-size: 14px;
	padding: 0 20px;
}
.logout{
	display: inline-block;
	color: #ffffff;
	width: 100px;
	line-height: 60px;
	font-size: 14px;
	border-left:1px solid #292d30;
}
.logout span{
	display: block;
	box-sizing: border-box;
	background:url(../../common/images/ic_out.png) 15px center no-repeat ;
	padding-left:50px ;
}
/*左部*/
.west{
	border: none;
	width:200px;
	border-right: 1px solid #CCCCCC;
	background-color: #f4f4f4;
	box-sizing: border-box;
	background-color: #ebebeb;
}
.menuUl{
	text-align: center;
	padding: 0 !important;
}
.menuUl li{
	position: relative;
	background-color: #F4F4F4;
	/*padding: 10px 0px !important;*/
	/*height: 50px;*/
}

.icon {
	display: block;
	width: 10px;
	height: 10px;
	position: absolute;
	right: 16px;
	top: 20px;
	background-image: url(../../common/images/nav_ic_more.png);
}
.menuUl li:hover{
	/*background-color: #ff8c40;*/
	/*color: #FFFFFF;*/
}
.menuItem,.submenuItem{
    padding-left: 50px;
	font-size: 16px;
	display: block;
	height: 50px;
	line-height: 50px;
	cursor: pointer;
    text-align: left;
}
.menuItemActive,.menuItem:hover{
	background-color: #ff8c40;
	text-decoration: none;
	color: #FFFFFF;
}
.submenu{
	display: none;
}
.submenuItem{
	background-color: #ebebeb;
}
.submenuItemActive,.submenuItem:hover{
	background-color: #e1e1e1;
	color: #222222;
}
.center{
    overflow: hidden;
	border: none;
}

/* 遮罩 */
#mask-top {
    display: none;
    height: 59px;
    width: 100%;
    background: #000000;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000;
    opacity: 0.4;
}
#mask-left {
    display: none;
    height: 100%;
    width: 199px;
    background: #000000;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000;
    opacity: 0.4;
}