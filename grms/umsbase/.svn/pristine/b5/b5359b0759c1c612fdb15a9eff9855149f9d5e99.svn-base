package com.hori.util.enums;

public enum OpenfireResultType {
	/**
	 * 保存成功
	 */
	RESULT_SUCCESS("成功",0),
	/**
	 * 保存失败
	 */
	RESULT_ERROR("错误",1),
	/**
	 * 其它错误
	 */
	RESULT_FAIL("失败",2);
	
	private OpenfireResultType(String name, int value) {
		this.name = name;
		this.value = value;
	}

	/**
     * 类型的值
     */
    private int value;

	/**
     * 名称
     */
    private String name;
    
    

    public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}


	
}
