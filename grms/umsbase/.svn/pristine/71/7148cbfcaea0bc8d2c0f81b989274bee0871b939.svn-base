    padding-bottom: 20px;
}

.add-area {
    width: 80px;
    height: 36px;
    vertical-align: middle;
    display: inline-block;
    box-sizing: border-box;
    border: 1px solid #FC883B;
    background-color: #fff;
    color: #FC883B;
    border-radius: 3px;
    line-height: 34px;
    text-align: center;
    margin-left: 20px;
}
.add-area {
    cursor: pointer;
}
