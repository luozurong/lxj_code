body{
	min-width: 1100px;
	box-sizing: border-box;
}
/*榛樿浜害*/

.brightness {
    height: 60px;
    width: 50%;
    float: left;
}

.text-1 {}

.text-1,
.text-2 {
    width: 94px;
    padding-left: 20px;
    height: 60px;
    line-height: 60px;
    font-size: 14px;
    color: #222222;
}
.volume .text-2{
    padding-left: 0;
}

.logo-1,.logo-2,.logo-3 {
    margin-top: 18px;
    width: 24px;
    height: 24px;
    background: url("../images/ic_light.png");
}

.slider-pattern1,
.slider-pattern2 {
    width: 130px;
    height: 12px;
    margin-top: 24px;
    /*margin-left: 30px;*/
    position: relative;
    background-color: #F2F2F2;
}

.slider-pattern1>div {
    height: 100%;
    width: 80%;
    background-color: #ccc;
}

.slider-pattern2>div {
    height: 100%;
    width: 90%;
    background-color: #ccc;
}

.slider-run1,
.slider-run2 {
    /*width: 400px;*/
    height: 12px;
    position: absolute;
    left: 0;
    top: 5px;
    background-color: #36b044;
}

.percent-1,
.percent-2 {
    margin-left: 20px;
    height: 60px;
}

.percent-1>div,
.percent-2>div {
    width: 21px;
    height: 60px;
    line-height: 60px;
    border: none;
    font-size: 14px;
    color: #6a6a6a;
    text-align: left;
}


/*闊抽噺*/

.volume {
    width: 50%;
    float: left;
    height: 60px;
}

.logo-2 {
    background: url("../images/ic_violin.png");
}
.logo-3 {
    background: url("../images/ic_voice.png");
}



body {
    padding-left: 24px;
    padding-bottom: 60px;
}

.title strong {
    color: red;
}

.header {
    position: relative;
    /*margin-left: 24px;*/
    height: 54px;
    border-bottom: 1px solid #eeeeee;
}
.goback {
    position: absolute;
    left: 0;
    top: 12px;
    padding-left: 20px;
    padding-right: 20px;
    /*width: 86px;*/
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    color: #fc883b;
    text-align: left;
    background: url(../../common/images/ic_back.png) left center no-repeat;
    cursor: pointer;
}
.goback-a {
    display: block;
    width: 86px;
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    color: #fc883b;
}
.header-edit {
    position: absolute;
    left: 76px;
    top: 17px;
    padding-left: 20px;
    /*width: 56px;*/
    /*height: 30px;*/
    /*line-height: 30px;*/
    font-size: 16px;
    color: #222222;
    text-align: left;
    border-left: 1px solid #CCCCCC;
}

.title {
    padding-top: 40px;
    padding-bottom: 10px;
}
.cl1-2 {
    /*display: inline-block;*/
    height: 26px;
    line-height: 26px;
    font-size: 14px;
    width: 10px;
    text-align: center;
    color: #6a6a6a;
}

#rule_text {
    width: 300px;
    height: 36px;
    border-radius: 3px;
    border: 1px solid #CECDCD;
    padding-left: 5px;
}

.content2_box {
    width: 1104px;
    /*height: 250px;*/
    border: 1px solid #CECDCD;
    border-bottom: none;
    box-sizing: border-box;
}
.rule-box {
    position: relative;
    width: 100%;
    border-bottom: 1px solid #CECDCD;
}
.rule-delete {
    position: absolute;
    right: 16px;
    top: 11px;
    width: 20px;
    height: 20px;
    background: url("../../homepage/images/btn_ic_cancle.png") no-repeat;
    background-size: cover;

}
.rule-delete:nth-child(1) {
    display: none;
}
.brightness_box {
    width: 100%;
    height: 60px;
    border-bottom: 1px solid #CECDCD;
    box-sizing: border-box;
}

.op_time_box,
.day_box,
.set_brightness_box {
    width: 100%;
    height: 63px;
    line-height: 63px;
    box-sizing: border-box;
    padding-left: 20px;
}

.op_time_box div {
    height: 100%;
    float: left;
}

.op_time_box .time_box {
    padding-left: 20px;
    padding-right: 20px;
}

.op_time_box .time_bt {
    width: 60px;
    height: 36px;
    border: 1px solid #F2F2F2;
    border-radius: 3px;
    box-sizing: border-box;
    margin-top: 13.5px;
}


/*.day_box{
				width: 100%;height: 63px;padding-left: 20px;box-sizing: border-box;line-height: 63px;
			}*/

#choose_yes {
    margin-top: 25px;
    margin-right: 14px;
    float: left;
    vertical-align: middle;
    width: 15px;
    height: 15px;

}

.day_box div {
    height: 100%;
    float: left;
}

.day_box .day_box1 {
    padding-right: 20px;
}

.day_box .day_box1,
.day_box .day_box2 {
    height: 36px;
