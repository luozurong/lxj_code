}

.ssa,
.music,
.audiomusic{
    width: 130px;
    height: 12px;
}

#liangdu,#yinliang,#audioyinliang,.num-deng,.num-yin,.liangdu,.yinliang,.audioyinliang{
    display: block;
    margin-top: 1px;
    margin-left: 1px;
    width: 32px;
    height: 30px;
    line-height: 30px;
    box-sizing: border-box;
    border: none;
    text-align: right;
    color: #6a6a6a;

}
.num-deng,.num-yin {
    display: block;
    width: 18px;
    line-height: 30px;
    box-sizing: border-box;
    border: none;
    text-align: center;
    color: #6a6a6a;

}
.box1 {
    margin-top: 16.5px;
    width: 58px;
    height: 32px;
    border: 1px solid #c2c2c2;
    border-radius: 3px;
}

.num_box {
    height: 100%;
    float: left;
    padding-left: 10px;
    padding-right: 24px;
}

#music_add,
#ssa_add,
#audiomusic_add,
.ssa_add,
.music_add,
.audiomusic_add
{
    position: absolute;
    top: 5px;
    width: 1px;
    border: 1px solid #34c745;
    height: 10px;
    z-index: 9;
    background-color: #34C745;
}

.logo-2,.logo-1,.logo-3 {
