
.cl1-1{
    display: inline-block;
    height: 26px;
    line-height: 26px;
    font-size: 14px;
    width: 10px;
    color: #eb6663;
    text-align: center;
}
.cl1 {
    min-width:80px;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    color: #222222;
    text-align: left;
}
.cl2 {
    min-width:60px;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    color: #222222;
    text-align: left;
}
.cl3 {
    margin-left: 40px;
    min-width:60px;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    color: #222222;
    text-align: left;
}
.textbox.combo {
    width: 130px !important;
}

.yunwei .textbox-text {
    width: 130px !important;
}
.the-bottom {
    position: absolute;
    width:100%;
    left:0;
    bottom: 0;
    z-index: 999;
    height:80px;
    border-top: 1px solid #f1f1f1;
}
/*角色名称*/
.role-name {
    margin-top: 20px;
    margin-left: 30px;
}

.role-name .cl2>input {
    padding-left: 2px;
    width: 416px;
    height: 32px;
    line-height: 32px;
    border: 1px solid #c9c9c9;
    border-radius: 4px;
    text-align: left;

}
/*角色描述*/
.role-detail {
    margin-left: 30px;
    margin-top: 10px;
}

.textarea-1 {
    line-height: 32px;
    padding-left: 2px;
    width: 416px;
    height: 32px;
    max-height: 32px;
    border: 1px solid #c9c9c9;
    border-radius: 4px;
    resize: none;
}
/*运维*/
.yunwei {
    margin-left: 30px;
    margin-top: 6px;
}
.choice {
    margin-left: 20px;
    width:40px;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    color: #222222;
    text-align: right;
    background: url("../images/circle.png") 2px 10px no-repeat;
}
.choiceActive {
    background: url("../images/ic-choice.png") 2px 10px no-repeat;
}

/*树*/
.tree-wrap {
    margin-left: 202px;
    margin-top: 6px;
}
#roleWin .tree-node-selected .tree-title{
    background: #FFFFFF;
    color: #222222;
}
.datagrid-btable{
    border-left:1px solid transparent !important;
    border-right:1px solid transparent;
    border-bottom:1px solid transparent;
}















