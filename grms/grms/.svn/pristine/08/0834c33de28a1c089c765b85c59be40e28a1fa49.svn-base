/*************分配工单******************/
#allotWorkOrder{
    color:#666;
}
.allotWorkOrder-input{
    margin-top:10px;
    margin-left:40px;
}
.allotWorkOrder-input input{
    width:115px;
    height:30px;
    box-sizing: border-box;
    padding: 0 5px;
    color:#666;
}
.allotWorkOrder-tabel{
    margin-left:38px;
    margin-top:10px;
}
.allotWorkOrder-input button{
	box-sizing:border-box;
	width:88px;
	height:32px;
	border:1px solid #51d2b6;
	color:#51d2b6;
	border-radius:5px;
	float:right;
	margin-right:40px;
}
#allotWorkOrder-tabel Table{
	float:right;
}
.l-btn{
	border-radius:0;
	-webkit-border-radius:0;
}
.allotWorkOrder .pagination span:hover{
	padding:1px;
}
#allotWorkOrder .datagrid-row{
	height: 32px !important;
}
#allotWorkOrder .datagrid{
	border-right:none;
}
#pp-allotWorkOrderTable,#pp-allotWorkOrderTable3{
	border:1px solid #eee;
	box-sizing:border-box;
	padding-bottom:5px;
	width:423px;
	padding-top:5px;
	margin-top:5px;
}
.pagination table {
  float: right;
  height: 30px;
}
#pp-allotWorkOrderTable2{
	margin-top:20px;
	padding:5px 0;
	margin-right:50px;
}
#pp-allotWorkOrderTable2>table{
	float:right !importent;
	
	
}
#allotWorkOrder .datagrid-wrap, #allotWorkOrder .datagrid-view{
	width:420px !important;
}
.datagrid-body::-webkit-scrollbar{
    height:0px;

}
.datagrid-body::-webkit-scrollbar-thumb{
    background: #fff;
}
#pp-allotWorkOrderTable2{
	border:1px solid #eee;
	width:423px;
	margin-bottom:30px;
}
/***************消息提示******************/
#withdrawWorkOrder,#msgWorkOrder,#msgWorkOrder2,#withdrawWorkOrder,#returnWorkOrder{
	display:table;
}
.msgWorkOrder-words{
	display:table-cell;
	vertical-align: middle;
    text-align: center;
    font-size:16px;
    padding:0 40px;
    box-sizing:border-box;
    width:100%;
}
/****************设备详情***********************/
.detailWorkOrder-choose{
    overflow: hidden;
    color:#666;
    /* width:100%; */
    box-sizing: border-box;
    /* padding:0 20px; */
}
.detailWorkOrder-choose>div{
    float:left;
}
.detailWorkOrder-choose>div span{
    display: block;
    width:80px;
    padding:10px  5px;
    margin:0 auto;
    text-align: center;
}
.detailWorkOrder-choose>div.detailWorkOrder-choose-active span{
    color:#51d2b6;
    border-bottom:2px solid #51d2b6;
}
.detailWorkOrder-item{
    margin-left:40px;
    margin-top:23px;
}
.detailWorkOrder-item>div{
     overflow: hidden;
     display: none;
 }
.detailWorkOrder-item>div:nth-child(1){
    overflow: hidden;
    display: block;
}
.detailWorkOrder-item>div.detailWorkOrder-item-active{
    display: block;
}
.detailWorkOrder-item>div>div{
    line-height: 36px;
    color:#666;
}
.detailWorkOrder-item>div>div span{
    display:block;
    float:left;
}
.detailWorkOrder-item>div>div span:nth-child(2){
    line-height:36px;
    color:#999;
    width: 545px;
}
/***************工单进度******************/
.progressWorkOrder-word{
    margin-top:35px;
    margin-left:70px;
    color:#666;
}
.progressWorkOrder-word>div{
    line-height:30px;
}

/*搜索输入框*/
.searchInput{
	font-size: 14px;
	width: 130px;
	height: 32px;
	box-sizing: border-box;
	border: 1px solid #C2C2C2;
}
/*记录弹窗*/
.recordWindowMask{
	position: fixed;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	z-index: 1;
	background-color: rgba(0,0,0,0.4);
	font-size: 16px;
}
.recordWindow{
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -290px;
	margin-top: -250px;
	width: 580px;
	height: 500px;
	background-color: #FFFFFF;
	border-radius: 4px;
}
.recordHeader{
	font-size: 18px;
	height: 50px;
	line-height: 50px;
	text-align: center;
	background-color: #51d2b6;
	color: #FFFFFF;
	border-radius: 4px 4px 0 0;
	
}
.recordInfo{
	padding: 20px;
}
.recordList,.otherInput{
	margin-bottom: 20px;
}
.recordList select,.otherInput input{
	display: inline-block;
	width: 416px;
	height: 32px;
	border: 1px solid #CCCCCC;
	color: #666666;
	border-radius: 2px;
}
.otherInput{
	/*margin-left: 103px;*/
	display: none;
}
.recordTxt{
	display: inline-block;
	width: 100px;
	text-align: right;
	vertical-align: middle;
}
.recordVal{
	color: #666666;
	font-size: 14px;
}
.recordImg img{
	width: 64px;
	height: 64px;
	margin: 5px;
}
.recordFooter{
	border-top: 1px solid #f1f1f1;
	padding-top: 20px;
	text-align: center;
}
.recordFooter>span{
	display: inline-block;
	width: 120px;
	height: 40px;
	border: 1px solid #51D2B6;
	color: #51D2B6;
	border-radius: 4px;
	line-height: 40px;
	cursor: pointer;
}
.closeIcon{
	display: inline-block;
	width: 16px;
	height: 16px;
	margin-top: 15px;
	margin-right: 20px;
	cursor: pointer;
	background: url(../../common/images/btn_cancle.png) left center no-repeat;
}
