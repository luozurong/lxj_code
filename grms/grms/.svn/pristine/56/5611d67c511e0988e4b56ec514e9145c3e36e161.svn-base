.shadeBox{
    display: none;
    position: fixed;
    width: 100%;
    height: 100%;
    /*background-color: #000000;*/
    background-color: rgba(0,0,0,.5);
    left: 0;
    top: 0;
    z-index: 999999;
}
.tipsInfo{
    position: absolute;
    width: 300px;
    /*height: 50px;*/
    top: 30%;
    left: 50%;
    margin-left: -250px;
    background-color: #FFFFFF;
    border-radius: 5px;
}

.tipsInfo p{
    text-align: center;
    line-height: 80px;
    color: #222;
}
#newStatistics{
    width:340px;
    height:160px;
    position: absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    z-index: 10;
    margin:auto;
    background: #eee;
    border-radius: 5px;
}
#newStatistics div{
    margin:63px auto 0;
    width:270px;
    text-align: center;

}
#newStatistics span{
    color:#555;
    font-size: 20px;
    margin-left:5px;
    display: inline-block;
    margin-left: 10px;
}
#newStatistics img{
    display: inline-block;
}

.checkbox-icon{
	position: relative;
	z-index: 1;
	background: url(../images/ic_choice-grey_nor.png) no-repeat 0  0px;
}
.checkbox-bg{
	position: relative;
	background: url(../images/ic_choice_green.png) no-repeat 0  0px;
	z-index: 1;
}
.radio-icon{
	position: relative;
	z-index: 1;
	background: url(../images/ic_choice-grey_nor2.png) no-repeat 0  0px;
}
.radio-bg{
	position: relative;
	background: url(../images/ic_choice_green2.png) no-repeat 0  0px;
	z-index: 1;
}
.readonly-bg{
	position: relative;
	background: url(../images/ic_!.png) no-repeat 0  0px !important;
	z-index: 2;
}
tr td .datadisable{
	background: url(../images/ic_!.png) no-repeat center  center;
}
label input[type="radio"],label input[type="checkbox"]{
	opacity: 0;
	margin-right: 10px;
}
.dataCheck{
	background: url(../images/ic_choice-grey_nor.png) no-repeat center center;
}
.dataChecked{
	background: url(../images/ic_choice_green.png) no-repeat center center;
}
.datagrid-cell input{
	opacity: 0;
}