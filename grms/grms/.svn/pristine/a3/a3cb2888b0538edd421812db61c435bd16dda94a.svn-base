html,body {
    background-color: #f8f8f8;
}


/*路径导航样式*/
.topTitle {
    height: 50px;
    background-color: #f8f8f8;
}

.path1,.path2 {
    padding-left: 28px;
    min-width: 60px;
    height: 50px;
    line-height: 50px;
    font-size: 14px;
    color:#666;
}
.path1 {
    margin-left: 24px;
    background: url("../images/ic_location.png") no-repeat left 15px;
}
.path2 {
    margin-left: 15px;
    background: url("../images/arrow.png") no-repeat left 20px;
}
.statCss{
	margin-top: 5px;
    margin-left: 20px;
}
.account {
	margin-top: 20px;
    margin-left: 16px;
    /*width: 60px;*/
}
.account1 {
    margin-top: 15px;
    width: 116px;
    height: 34px;
}
.account1>input {
    width: 114px;
    height: 32px;
    border: 1px solid #c2c2c2;
}
.probtn{
	margin-top: 10px;
    margin-left: 20px;
}
.create, .search,.delete {
    margin-top: 5px;
    width: 88px;
    height:30px;
    margin-left: 60px;
    line-height: 25px;
    font-size: 14px;
    color:#97e4d3;
    text-align: center;
    border: 1px solid #97e4d3;
    background-color: #FFFFFF;
    border-radius: 3px;
    cursor: pointer;
}
.wrap{
	margin: 20px;
}
.body-box{
	margin-left: 50px;
}
.table-wrap{
	width: 100%;
	overflow: hidden;
}
.table-wrap>div{
	width: 30%;
	float: left;
	line-height: 50px;
}
.table-wrap>div.table-width{
	width: 50%;
}
.table-wrap-three{
	overflow:hidden;
	margin-top:50px;
}
.table-wrap-three span{
	float:left;
	line-height: 50px;
}
.table-wrap-three>div{
	float: left;
	line-height:50px;
	width:70%;
}


.projectProductList_Box .datagrid-row-checked{
  background-color:#fff!important;
}