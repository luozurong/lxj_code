//更新附件url
function updateUrl(id,url){
	//查找出框架合同的其他执行合同
	 $.ajax({
           type : "POST",  //提交方式
           url : "/grms/contract/updateContractUrl",//路径
           async: false,
           data:  {"id":id,"url":url},
           dataType : "json",//数据，这里使用的是Json格式进行传输
           success : function(result) {//返回数据根据结果进行相应的处理
               if ( result.success) {
                   infoMask("更新附件成功");
                   findDepart('','');
               } else {
                   infoMask("更新附件失败");

               }
           }
       });
}
