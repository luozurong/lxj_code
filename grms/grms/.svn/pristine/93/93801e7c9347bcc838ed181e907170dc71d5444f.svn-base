        type : "POST",  //提交方式  
        url : "/grms/contract/approveContract",//路径  
        data: {"id":id,"desc":desc,"status":status} ,  
        dataType : "json",//数据，这里使用的是Json格式进行传输  
        success : function(result) {//返回数据根据结果进行相应的处理  
            if (result.success) {  
            	if(status=='y'){
                    $('#roleWin').window('close');
                    $('#approveY').val('');
            	}else{
                    $('#roleWin2').window('close');
                    $('#approveN').val('');
