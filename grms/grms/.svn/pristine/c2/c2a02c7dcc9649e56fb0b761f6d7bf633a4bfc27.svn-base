		PendingEvent pe = new PendingEvent();
		pe.setId(UUIDGenerator.generate());
		pe.setProjectName(projectName);
		pe.setProjectCode(projectCode);
		pe.setModelCode(projectCode);
		pe.setModelName("项目管理");
