html,body {
    background-color: #f8f8f8;
}
.wrap{
	margin: 0 30px;
	background: #fff;
	padding: 0 20px 20px;
}
.workorder-breadcrumb{
	overflow:hidden;
	line-height: 45px;
	margin-left: 30px;
}
.workorder-breadcrumb span{
	font-size: 14px;
}
/*路径导航样式*/
.topTitle {
    height: 50px;
    background-color: #f8f8f8;
}

.path1,.path2 {
    padding-left: 28px;
    min-width: 60px;
    height: 50px;
    line-height: 50px;
    font-size: 14px;
    color:#666;
}
.path1 {
    margin-left: 24px;
    background: url("../images/ic_location.png") no-repeat left 15px;
}
.path2 {
    margin-left: 15px;
    background: url("../images/arrow.png") no-repeat left 20px;
}
.statCss{
	margin-top: 5px;
    margin-left: 20px;
}
.account {
	margin-top: 20px;
    margin-left: 16px;
    /*width: 60px;*/
}
.account1 {
    margin-top: 15px;
    width: 116px;
    height: 34px;
}
.account1>input {
    width: 114px;
    height: 32px;
    border: 1px solid #c2c2c2;
}
.probtn{
	margin-top: 10px;
    margin-left: 20px;
}
.search{
    margin-top: 0px;
    width: 90px;
    height:27px;
    margin-left: 60px;
    line-height: 25px;
    font-size: 14px;
    color:#97e4d3;
    text-align: center;
    border: 1px solid #97e4d3;
    background-color: #FFFFFF;
    border-radius: 3px;
    cursor: pointer;
    background:#fff;
}
.l-btn:hover{
	color:#97e4d3;
    text-align: center;
    border: 1px solid #97e4d3;
    background: #fff;
}
.l-btn-focus{
	outline:none;
}
.goback{
    margin-top: 15px;
    width: 88px;
    height:30px;
    margin-left: 10px;
    line-height: 25px;
    font-size: 14px;
    color:#000000;
    text-align: center;
    border: 1px solid #000000;
    background-color: #c0c0c0;
    border-radius: 3px;
    cursor: pointer;
}
.handle{
    margin: 0 40px;
    width: 88px;
    height:32px;
    line-height: 25px;
    font-size: 14px;
    color:#000000;
    text-align: center;
    border: 1px solid #000000;
    background-color: #c0c0c0;
    border-radius: 3px;
    cursor: pointer;
    margin-top: 8px;
    line-height: 30px;
}
.pagination table{
	float: right;
}
.conditions-select{
	backgroun: #fff;
}
#businessType,#status{
	height: 32px;
}
#pp{
	border: 1px solid #eee;
	margin: 20px  0;
}
.datagrid-header-row td,.datagrid-header-row td:hover{
	border-color: #4d525a;
	background: #4d525a;
	color: #fff;
}
.datagrid-body td{
	border: 1px solid transparent;
}
.datagrid-btable tbody tr:nth-child(even){
	background: #f8f8f8;
}
/*表格内A标签*/
.datagrid-cell>a{
    padding:0 14px;
    border-right:1px solid #ccc;
    color:#51d2b6;
}
.datagrid-cell>a:first-child{
    padding-left: 0;
}
.datagrid-cell>a:last-child{
    border-right:none;
}
.exception-btn{
	width: 400px;
	border-top: 1px solid #eee;
	display: table-cell;
	vertical-align: middle;
}
.dialog-button a{
	background: #fff;
}
#finance{
	padding: 10px;
}
#finance .word{
	display:inline-block;
	margin-left: -90px;
}
#resultRemark{
	height: 90px;
}
.datagrid-cell, .datagrid-cell-group, .datagrid-header-rownumber, .datagrid-cell-rownumber{
	word-break: break-all;
}
#searchBtn .l-btn-text{
	line-height: 28px;
}