var dataStatus = null;
var pageNoAll='1';
var pageSizeAll='10';
$(function(){
	//初始化项目状态选择值
	dataStatus = -2;
	
	//格式化时间
    $(".datebox-1").datebox({
        value : '',
        editable : false,
        onSelect : function(startTime){
            $('.datebox-2').datebox('calendar').calendar({
                validator: function(date){
                    return startTime<=date;//<=
                }
            });
        }
    });
    //格式化时间
    $(".datebox-2").datebox({
        value : '',
        editable : false,
        onSelect : function(endTime){
            $('.datebox-1').datebox('calendar').calendar({
                validator: function(date){
                    return endTime>=date;//<=
                }
            });
        }
    });
	
    selectAll();
	
	
})

function selectAll(){
	pageNoAll='1';
	  $('#pp').pagination({
	        pageNumber:1
	     
	    });
	  search('','');
}

//查询数据
function search(pageSize,pageNo){
	var key=$('#key').val();
	var startTime= $(".datebox-1").datebox('getValue');
	var endTime=$(".datebox-2").datebox('getValue');
	
	pageNo=pageNoAll;
	pageSize=pageSizeAll;
	var pendingEventQueryBean='{"key":"'+key+'","startTime":"'+startTime+'","endTime":"'+endTime
	+'","pageNo":"'+pageNo+'","pageSize":"'+pageSize+'"}';
	
    $.ajax({  
        type : "POST",  //提交方式  
        url : "/grms/pendingEvent/getListData.html",//路径  
        dataType : "json",//数据，这里使用的是Json格式进行传输  
        data:  {"pendingEventQueryBean":pendingEventQueryBean},  
        success : function(result) {//返回数据根据结果进行相应的处理  
            if ( result.success) {  
            	var data2=result.rows;
            	var total=result.total;
            	refresh(data2);
            	paginationpage(total,data2);
            	
            	
            } else {  
            	
            }  
        }  
    }); 
}

