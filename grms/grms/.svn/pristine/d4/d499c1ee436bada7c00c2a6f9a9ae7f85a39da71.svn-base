html,body {
    background-color: #f8f8f8;
}
.datagrid-header td, .datagrid-body td, .datagrid-footer td {
    border-width: 0;
}
.datagrid-htable tbody td {
    color: #fff;
}
.datagrid-header td.datagrid-header-over{
    background:#4d525a;
    color:#fff;
}
.datagrid-btable tbody tr:nth-child(even){
    background:#f8f8f8;
}
.datagrid-htable, .datagrid-btable, .datagrid-ftable{
    color:#666;
}
.datagrid-header-row td {
    background: #4d525a;
}
/*表格内A标签*/
.datagrid-cell>a{
    padding:0 14px;
    border-right:1px solid #ccc;
    color:#51d2b6;
}
.datagrid-cell>a:first-child{
    padding-left: 0;
}
.datagrid-cell>a:last-child{
    border-right:none;
}

/*路径导航样式*/
.topTitle {
    height: 50px;
    background-color: #f8f8f8;
}

.path1,.path2 {
    padding-left: 28px;
    min-width: 60px;
    height: 50px;
    line-height: 50px;
    font-size: 14px;
    color:#666;
}
.path1 {
    margin-left: 24px;
    background: url("../images/ic_location.png") no-repeat left 15px;
}
.path2 {
    margin-left: 15px;
    background: url("../images/arrow.png") no-repeat left 20px;
}
.statCss{
	margin-top: 5px;
    margin-left: 20px;
}
.account {
	margin-top: 20px;
    margin-left: 16px;
    /*width: 60px;*/
}
.account1 {
    margin-top: 15px;
    width: 116px;
    height: 34px;
}
.account1>input {
    width: 114px;
    height: 32px;
    border: 1px solid #c2c2c2;
}
.probtn{
	margin-top: 10px;
    margin-left: 20px;
}
.create, .search,.delete {
    margin-top: 9px;
    width: 88px;
    height:30px;
    margin-right: 30px;
    margin-left: 30px;
    line-height: 25px;
    font-size: 14px;
    color:#51d2b6;
    text-align: center;
    border: 1px solid #51d2b6;
    background: #fff;
    border-radius: 3px;
    cursor: pointer;
}
.stopProject{
    margin-top: 15px;
    width: 88px;
    height:30px;
    line-height: 25px;
    font-size: 14px;
    color:#000000;
    text-align: center;
    border: 1px solid #000000;
    background-color: #c0c0c0;
    border-radius: 3px;
    cursor: pointer;
}
.pagination table{
	float: right;
}

/*自定义*/
.conditions-select{
	width: 100%;
    box-sizing: border-box;
    padding:0 20px;
}
.projectStautsBox{
	background: #fff;
	margin-top:0;
}
.workorder-tabs{
    overflow: hidden;
	border-bottom:none;
   /*  border-bottom: 1px solid #ccc; */
    line-height:45px;
    font-size: 14px;
}
.workorder-select{
    border-bottom:3px solid #51d2b6;
    color:#51d2b6;
}
.workorder-tabs>div{
    float:left;
    padding-left:23px;
    padding-right:23px;
    position: relative;
}
.accountBox{
	margin-top: 20px;
	background: #fff;	
	padding-bottom:20px;
	padding-right:20px;
	width: 100%;
    box-sizing: border-box;
}
.accountBox2{
	display: inline-block;
}
.account span{
	width: 75px;
    height: 34px;
    line-height: 34px;
    display: inline-block;
    vertical-align: top;
    font-size: 14px;
}
.account input{
	width: 114px;
    height: 32px;
    border: 1px solid #c2c2c2;
}
.searchBox{
	border-top:none;
	background: #fff;
	border-top: 1px solid #eee;
	padding: 10px 0;
}
.searchBox .l-btn-text{
	line-height:28px;
}
.content-wrap{
	box-sizing: border-box;
    padding:20px 20px 0;
}
.l-btn:hover{
	background: #fff;
	color: #51d2b6;
	border-color: #51d2b6;
}