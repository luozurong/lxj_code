html,body {
    background-color: #f8f8f8;
}


/*路径导航样式*/
.topTitle {
    height: 50px;
    background-color: #f8f8f8;
}

.path1,.path2 {
    padding-left: 28px;
    min-width: 60px;
    height: 50px;
    line-height: 50px;
    font-size: 14px;
    color:#666;
}
.path1 {
    margin-left: 24px;
    background: url("../images/ic_location.png") no-repeat left 15px;
}
.path2 {
    margin-left: 15px;
    background: url("../images/arrow.png") no-repeat left 20px;
}
.statCss{
	margin-top: 5px;
    margin-left: 20px;
}
.account {
	margin-top: 20px;
    margin-left: 16px;
    /*width: 60px;*/
}
.account1 {
    margin-top: 15px;
    width: 116px;
    height: 34px;
}
.account1>input {
    width: 114px;
    height: 32px;
    border: 1px solid #c2c2c2;
}
.probtn{
	margin-top: 10px;
    margin-left: 20px;
}
.create, .search,.delete {
    margin-top: 5px;
    width: 88px;
    height:30px;
    margin-left: 60px;
    line-height: 25px;
    font-size: 14px;
    color:#97e4d3;
    text-align: center;
    border: 1px solid #97e4d3;
    background-color: #FFFFFF;
    border-radius: 3px;
    cursor: pointer;
}
#hintStatistics>div{
    text-align: center;
    line-height: 280px;
    font-size: 20px;
    color:#666;
}
#exportStatistics{
	display:table;
}
#exportStatistics>div{
	display:table-cell;
	vertical-align: middle;
	font-size:16px;
	padding:0 40px;
    text-align: center;
}
.body-wrap .content-wrap{
	margin-left: 229px;
}
#pp{
	margin-left:1px;
	border: 1px solid #eee;
	border-right: none;
	border-left: none;
}
.body-wrap{
	/* margin-top: 30px; */
}
.search-btn{
	background: #fff;
	margin: 24px 24px 0 24px;
	padding: 24px;
}
#condition{
	width: 190px;
	height: 32px;
	line-height: 32px;
	border-radius: 3px;
	border:1px solid #ccc;
	padding-left: 10px;
	margin-right: 40px;
}
#search,#create{
	display:inline-block;
	width: 100px;
	height: 32px;
	text-align: center;
	border: 1px solid #51d2b6;
	color: #51d2b6;
	line-height: 32px;
	border-radius: 3px;
	vertical-align: bottom;
	margin-right: 10px;
	cursor: pointer;
}
#batchDel{
	display:inline-block;
	width: 100px;
	height: 32px;
	text-align: center;
	border: 1px solid #ff4747;
	color: #ff4747;
	line-height: 32px;
	border-radius: 3px;
	vertical-align: bottom;
	cursor: pointer;
}