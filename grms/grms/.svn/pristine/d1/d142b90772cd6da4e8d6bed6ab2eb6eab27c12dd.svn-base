/*  CSS reset  ��ʽ����    */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,p,th,td,input,select,textarea,button {margin:0;padding:0;font-family: "microsoft yahei";}
dl,ul,ol,li {
    list-style:none;
    padding:0;
    margin:0;
}
button {border:0 none;background-color:transparent;cursor:pointer}  /* ���ñ?button��ťЧ�� */
input,select,textarea,button {vertical-align:middle}  /* ���ñ?�ؼ���ֱ����*/
a,a:active, a:hover {text-decoration:none}   /* ��������a��ǩ����껬��Ч�� */
img {border:none;vertical-align:top}
input::-ms-clear, input::-ms-reveal {
    display: none;/*ȥ��IE��inputĬ�ϵĲ�ź��۾�*/
}
input,button,select,textarea{outline:none}/*ȡ��chrome��input��textarea�ľ۽��߿�*/
textarea{resize:none}/*ȡ��chrome��textarea���϶��Ŵ�*/



