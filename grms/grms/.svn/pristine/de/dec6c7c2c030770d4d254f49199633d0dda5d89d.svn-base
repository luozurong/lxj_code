    <!-- 查询待办事项列表 -->
    <select id="listForPendingEvent" resultType="java.util.Map" parameterType="com.hori.grms.queryBean.PendingEventQueryBean" >
        select 
        id, project_name projectName, project_code projectCode, model_name modelName, status status, create_time createTime, 
    creator_name creatorName ,url url
        from pending_event
        where is_show = 1
        	<if test="startTime != null  and startTime != '' and endTime != null and endTime != ''" >
				and <![CDATA[  create_time >= #{startTime} and create_time <= #{endTime}  ]]>	
			</if>
			<if test="key!= null and key != ''" >
				and ( project_name like concat('%',#{key},'%')  or model_name like concat('%',#{key},'%')) 
            </if>
			<if test="roleType != -1 and roleType != 8" >
				and role_type = #{roleType}
            </if>
			<if test="roleType = 0 and account!= null and account!=''" >
				and creator_account = #{account}
            </if>
            
            order by create_time desc
    </select>
    
    
