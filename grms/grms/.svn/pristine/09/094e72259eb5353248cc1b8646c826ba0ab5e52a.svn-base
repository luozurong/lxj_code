/*树*/
.tree-wrap {
    margin-left: 202px;
    margin-top: 6px;
}
.navigation{
	margin: 28px 30px;
}
.navigation a{
	height: 20px;
	line-height: 20px;
	font-size: 16px;
	display: inline-block;
	width: 70px;
	color: #666;
	margin-right: 20px;
	text-align: center;
}

 .navigation .nav-active{
	border-bottom:3px solid #51d2b6 ;
	color: #51d2b6;
}
 .content-box {
     height: 330px;
     overflow: auto;
     padding: 18px 30px 10px 16px;
 }
.content-box ul{
    padding: 0 20px 30px 0;
}
.content-box li{
	text-align: right;
	/*height: 32px;*/
	line-height: 32px;
	margin-top: 12px;
	
}
.content-box input,.content-box select{
	width: 388px;
	padding: 0 5px;
	height: 32px;
	line-height: 32px;
	    margin-left: 10px;
	   border: 1px solid #999;
	   border-radius: 2px;
}
.content-box select{
	width: 400px;
	padding: 0;
}
.content-box label{
	display: inline-block;
	vertical-align: top;
}
.content-box textarea{
	width: 388px;
    height: 40px;
	padding: 5px 5px;
	 margin-left: 10px;
	   border: 1px solid #999;
	   border-radius: 2px;
}
#content_box_2 li:nth-child(-n+5):before{
	content: "*";
	color: red;
	line-height: 150%;
}
#content_box_1 li:nth-child(-n+3):before{
	content: "*";
	color: red;
	line-height: 150%;
}
.special:before{
	content: "*";
    color: red;
    line-height: 222%;
    vertical-align: top;
}
.confirmBox{
    width: 500px;
    margin: 70px 40px 0;
