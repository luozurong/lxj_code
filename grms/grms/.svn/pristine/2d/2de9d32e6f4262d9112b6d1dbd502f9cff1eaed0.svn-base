/***************濞戝牊浼呴幓鎰仛瀵顢�****************/
.allMsg-top{
	background:#51d2b6;
}
.allMsg{
	position: fixed;
	bottom:20px;
	right:20px;
	z-index:10;
	background:#fff;
	border-radius:4px 4px 0 0;
}
.allMsg-top{
	background:#51d2b6;
	height:60px;
}
.allMsg-top{
	display: inline-block;
	width: 100%;
	text-align: center;
	line-height: 60px;
	color: #fff;
	border-radius: 6px 6px 0 0;
	position: relative;
}
.allMsg-top img{
	position: absolute;
	top:21px;
	right:20px;
	z-index: 11;
    cursor: pointer;
}
.allMsg-main{
	height:100px;
	width:300px;
	box-sizing: border-box;
	padding:0 20px;
	background: #fff;
	font-size: 14px;
	text-align: left;
	position: relative;
	display:table;
}
.allMsg-main span{
	display:table-cell;
	vertical-align: middle;
	text-align:center;
}
.allMsg-botton{
	height:60px;
	width: 100%;
	background: #fff;
	border-top:1px solid #eee;
	border-radius: 10px;
}
.allMsg-botton a{
	display: block;
	width:120px;
	height:40px;
	background: #51d2b6;
	border-radius: 5px;
	text-align: center;
	color: #fff;
	margin: 10px auto 0;
	line-height: 40px;
	cursor:pointer;
}
#allNum{
	-webkit-transform:scale(0.8);
}

