.menuUl{
	text-align: center;
	padding: 0 !important;
}
.menuUl li{
	position: relative;
	color: #848993;
	background-color: #2c323e;
	/*padding: 10px 0px !important;*/
	/*height: 50px;*/
}

.icon {
	display: block;
	width: 20px;
	height: 20px;
	position: absolute;
	right: 16px;
	top: 20px;
	background: url(../../common/images/nav_ic_more1.png) 0 0 no-repeat;
}
.menuItem {
    text-align: left;
    padding-left:54px;
}
.menuUl li{
	background-color: #2c323e;
	color: #848993;
}
.menuItem,.submenuItem{
	font-size: 16px;

	display: block;
	height: 50px;
	line-height: 50px;
	cursor: pointer;
	color: #848993;
}
.menuItemActive,.menuItem:hover{
	background-color: #202531;
	text-decoration: none;
	color: #51d2b6;
}
.submenu{
	display: none;
}
.submenuItem{
    padding-left: 54px;
    text-align: left;
	background-color: #202531;
	color: #FFFFFF;
	white-space: nowrap;
    overflow:  hidden;
    text-overflow: ellipsis;
    -ms-text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
}
.submenuItemActive,.submenuItem:hover{
	/*background-color: #e1e1e1;*/
	color: #51d2b6;
}
.center{
    overflow: hidden;
	border: none;
}
.open {
    background-color: #202531;
    text-decoration: none;
	color: #FFFFFF;
}

.left-border {
	position: absolute;
	left:0;
	top: 0;
	width:4px;
	height:50px;
	/*background-color: #51d2b6;*/
}
.left-border-check {
	background-color: #51d2b6;
}

/* 闁喚鍍� */
#mask-top {
    display: none;
    height: 60px;
    width: 100%;
    background: #000000;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000;
    opacity: 0.4;
}
#mask-left {
    display: none;
    height: 100%;
    width: 200px;
    background: #000000;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000;
    opacity: 0.4;
}
/*************瀹搞儱宕熷☉鍫熶紖瀵懓鍤锟�***************/
.mesNum-pop{
	position: fixed;
	top:56px;
	right:50px;
	z-index: 10;
	background:#fff;
	width:180px;
	border:1px solid #e4e4e4;
	display: none;
}
.mesNum-pop>div{
	line-height:40px;
	box-sizing: border-box;
	padding-left:20px;
	font-size: 14px;
	cursor: pointer;
}
.mesNum-pop>div:hover{
	background: #e1fef8;
}
.mesNum-pop .mesnum-san {
	position: absolute;
	top:-15px;
	left:110px;
	z-index: 10;
}
.mesNum-pop .mesnum-san:hover{
	background: transparent;
}
.mesNum-pop .mesnum-san img{
	margin-top:2px;
}
.mesNum-pop .set{
	padding-left:0;
	border-top:1px solid #e4e4e4;
}
.mesNum-pop  span:nth-child(2){
	display: inline-block;
	width: 23px;
	height:14px;
	border-radius: 7px;
	background: #51d2b6;
	line-height: 14px;
	float:right;
	margin-right:20px;
	margin-top:14px;
}
.mesNum-pop  span i{
	display: inline-block;
	width: 100%;
	height:14px;
	text-align: center;
	-webkit-transform:scale(0.65);
	font-style: normal;
	color:#fff;
}
.mesNum-pop .set:hover{
	background: #fff;
}
.mesNum-pop .set span{
	display: inline-block;

	width: 100%;
	text-align: center;
}
